{
  "name": "grammar-selector",
  "version": "0.48.2",
  "main": "./lib/main",
  "description": "Select the grammar to use for the current editor with `ctrl-shift-L`.",
  "license": "MIT",
  "repository": "https://github.com/atom/grammar-selector",
  "engines": {
    "atom": "*"
  },
  "dependencies": {
    "atom-select-list": "0.0.6",
    "atom-space-pen-views": "^2.0.0",
    "underscore-plus": "^1.6.2"
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "devDependencies": {
    "coffeelint": "^1.9.7"
  },
  "configSchema": {
    "showOnRightSideOfStatusBar": {
      "type": "boolean",
      "default": true,
      "description": "Show the active pane item's langauge on the right side of Atom's status bar, instead of the left."
    }
  }
}
